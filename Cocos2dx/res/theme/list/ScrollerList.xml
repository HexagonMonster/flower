<f:Scroller width="150" height="300" class="ScrollerList" xmlns:f="flower">
    <f:script src="./ScrollerList.js"/>
    <f:RectUI percentWidth="100" percentHeight="100" fillColor="0x888888"/>
    <f:List id="viewport" class="List">
        <f:itemRenderer>
            <f:ItemRenderer width="150" height="30">
                <f:Label text="{data.label}" horizontalCenter="0" verticalCenter="0"/>
            </f:ItemRenderer>
        </f:itemRenderer>
    </f:List>
</f:Scroller>