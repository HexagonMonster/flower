<f:List class="LabelList" selectTime="touch_end" xmlns:f="flower">
    <f:itemRenderer>
        <f:ItemRenderer class="LabelItemRenderer" percentWidth="100" height="30">
            <f:Rect percentWidth="100" percentHeight="100" fillColor.down="0xd6d4d0"
                      fillColor.selectedDown="0x64834e" fillColor.selectedUp="0x96b97d" visible.up="false"
                      visible.down="true" visible.selectedDown="true"
                      visible.selectedUp="true"/>
            <f:Label text="{data.label}" horizontalCenter="0" verticalCenter="0" fontColor="0x000000"/>
        </f:ItemRenderer>
    </f:itemRenderer>
</f:List>