var ServerMonster = (function () {

    function ServerMonster(id, data, dungeonData, callObject, triggerRunning) {
        this.id = id;
        this.data = data;
        this.dungeonData = dungeonData;
        this.callObject = callObject;
        this.triggerRunning = triggerRunning;

        this.x = data.bornCoord.x.value;
        this.y = data.bornCoord.y.value;

        this.callObject.addMonster(this.id, data.id.value, data.tag.value, this.x, this.y);
    }

    ServerMonster.prototype.update = function (dt) {

    }

    ServerMonster.prototype.dispose = function () {
        this.callObject.removeRole(this.id);
    }

    return ServerMonster;
})();

module.ServerMonster = ServerMonster;